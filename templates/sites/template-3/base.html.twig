<!DOCTYPE html>
<!--[if lt IE 7]>
<html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>
<html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>
<html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js"> <!--<![endif]-->
<head>
    <meta charset="utf-8">
    <!--[if IE]>
    <meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'><![endif]-->
    <title>{% block title %}{% endblock %}</title>
    <meta name="description" content="{% block description %}{% endblock %}">

    <!--[if lt IE 9]>
    <script type="text/javascript" src="js/ie-fixes.js"></script>
    <link rel="stylesheet" href="css/ie-fixes.css">
    <![endif]-->

    <meta name="description" content="Kanzi HTML5 Template">

    <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet"
          href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/revolution_settings.css') }}">
    <link rel="stylesheet" href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/bootstrap.css') }}">
    <link rel="stylesheet"
          href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/font-awesome.css') }}">
    <link rel="stylesheet" href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/axfont.css') }}">
    <link rel="stylesheet" href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/tipsy.css') }}">
    <link rel="stylesheet" href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/prettyPhoto.css') }}">
    <link rel="stylesheet"
          href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/isotop_animation.css') }}">
    <link rel="stylesheet" href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/animate.css') }}">
    <link href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/style.css') }}" rel='stylesheet'
          type='text/css'>
    <link href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/responsive.css') }}" rel='stylesheet'
          type='text/css'>
    <style type="text/css">
        :root {
            --colorTheme: {{ site.colorTheme }};
        }
    </style>
    <link href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/izibrick.css') }}"
          rel='stylesheet' type='text/css' id="skin-file">
    <script src="https://maps.googleapis.com/maps/api/js?v=3.exp&amp;sensor=false"></script>

    <!-- Fonts -->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,600' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Raleway:400,100,200,300,500,600' rel='stylesheet'
          type='text/css'>

    <!--[if lt IE 9]>
    <script type="text/javascript"
            src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/respond.js') }}"></script>
    <![endif]-->
</head>
<body>
<div id="wrapper">

    <div class="top_wrapper">

        <!-- Header -->
        <header id="header">
            <div class="container">

                <div class="row header">

                    <!-- Logo -->
                    <div class="col-xs-2 logo">
                        <a href="{{ path('home') }}">
                            <img src="{{ vich_uploader_asset(site, 'logoFile') | imagine_filter('logo') }}"
                                 alt="Logo {{ site.name }}"/>
                        </a>
                    </div>
                    <!-- //Logo// -->


                    <!-- Navigation File -->
                    <div class="col-md-10">

                        <!-- Mobile Button Menu -->
                        <div class="mobile-menu-button">
                            <i class="fa fa-list-ul"></i>
                        </div>
                        <!-- //Mobile Button Menu// -->

                        <nav>
                            <ul class="navigation">
                                <li>
                                    <a class="{% block indexActive %}{% endblock %}"
                                       href="{{ path('home') }}">
                                        <span class="label-nav">
                                        Accueil
                                                </span>
                                    </a>
                                </li>
                                <li>
                                    <a class="{% block presentationActive %}{% endblock %}"
                                       href="{{ path('presentation') }}">
                                        <span class="label-nav">
                                        Présentation
                                                </span>
                                    </a>
                                </li>
                                <li>
                                    <a class="{% block blogActive %}{% endblock %}"
                                       href="{{ path('blog') }}">
                                        <span class="label-nav">
                                        Blog
                                                </span>
                                    </a>
                                </li>
                                <li>
                                    <a class="{% block devisActive %}{% endblock %}"
                                       href="{{ path('devis') }}">
                                        <span class="label-nav">
                                        Devis
                                                </span>
                                    </a>
                                </li>
                                <li>
                                    <a class="{% block contactActive %}{% endblock %}"
                                       href="{{ path('contact') }}">
                                        <span class="label-nav">
                                        Contact
                                                </span>
                                    </a>
                                </li>
                            </ul>

                        </nav>
                    </div>
                    <!-- Nav -->

                </div>


            </div>
        </header>
    </div><!--.top wrapper end -->

    <div class="loading-container">
        <div class="spinner">
            <div class="double-bounce1"></div>
            <div class="double-bounce2"></div>
        </div>
    </div>


    <div class="content-wrapper hide-until-loading">
        {% block body %}    {% endblock %}
    </div><!--.content-wrapper end -->
    <footer>
        <div class="footer">

            <div class="container">
                <div class="footer-wrapper">
                    <div class="row">


                        <!-- Footer Col. -->
                        <div class="col-md-6 col-sm-6 footer-col">
                            <div class="footer-content">
                                <div class="footer-content-logo">
                                    <img src="{{ vich_uploader_asset(site, 'logoFile') | imagine_filter('logo') }}"
                                         alt="Logo {{ site.name }}"/>
                                </div>
                                <div class="footer-content-text">
                                    <p>{{ site.name }}</p>
                                </div>
                            </div>
                        </div>
                        <!-- //Footer Col.// -->


                        <!-- Footer Col. -->
                        <div class="col-md-3 col-sm-3 footer-col">
                            <div class="footer-title">
                                Links
                            </div>
                            <div class="footer-content">
                                <ul class="footer-category-list">
                                    <li>
                                        <a href="{{ path('home') }}">Accueil</a>
                                    </li>
                                    <li>
                                        <a href="{{ path('presentation') }}">Présentation</a>
                                    </li>
                                    <li>
                                        <a href="{{ path('blog') }}">Blog</a>
                                    </li>
                                    <li>
                                        <a href="{{ path('devis') }}">Devis</a>
                                    </li>
                                    <li>
                                        <a href="{{ path('contact') }}">Contact</a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <!-- //Footer Col.// -->

                    </div>
                </div>

            </div>
            <div class="copyright">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12 col-sm-12 center-text">
                            <div class="copyright-text">&copy; {{ "now"|date('Y') }} {{ site.domain }} | Développé par
                                <a
                                        href="http://www.ara-solutions.com" target="_blank">ARA Solutions</a></div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </footer>

</div><!-- wrapper end -->

<script type="text/javascript"
        src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/_jq.js') }}"></script>
<script type="text/javascript"
        src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/_jquery.placeholder.js') }}"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/activeaxon_menu.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/animationEnigne.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/bootstrap.min.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/ie-fixes.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/jq.appear.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/jquery.base64.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/jquery.carouFredSel-6.2.1-packed.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/jquery.cycle.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/jquery.cycle2.carousel.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/jquery.easing.1.3.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/jquery.easytabs.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/jquery.infinitescroll.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/jquery.isotope.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/jquery.prettyPhoto.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/jQuery.scrollPoint.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/jquery.themepunch.plugins.min.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/jquery.themepunch.revolution.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/jquery.tipsy.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/jquery.validate.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/jQuery.XDomainRequest.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/kanzi.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/retina.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/timeago.js') }}"
        type="text/javascript"></script>
<script src="{{ asset('templates/template-' ~ site.template.id ~ '/assets/js/tweetable.jquery.js') }}"
        type="text/javascript"></script>
</body>
</html>