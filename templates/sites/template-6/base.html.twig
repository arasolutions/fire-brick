<!DOCTYPE html>

<html class="{{  site.displayBoxed == true ? 'boxed' : '' }}">
<head>
    <meta charset="utf-8">
    <title>{% block title %}{{ site.name }}{% endblock %}</title>
    <meta name="keywords" content="HTML5 Template"/>
    <meta name="description" content="{% block description %}{% endblock %}">
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    {% if site.favicon %}
        <link rel="icon" type="image/png" href="{{ vich_uploader_asset(site, 'faviconFile') | imagine_filter('favicon') }}" />
    {% else %}
        <link rel="icon" type="image/png" href="{{ asset('/assets/img/logo/favicon.png') }}" />
    {% endif %}
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- Web Fonts  -->
    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600" rel="stylesheet">

    <!-- Libs CSS -->
    <link href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/style.css') }}" rel="stylesheet"/>
    <link href="{{ asset('assets/common/bootstrap/css/bootstrap.min.css') }}"
          rel="stylesheet"/>
    <link href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/font-awesome.min.css') }}"
          rel="stylesheet"/>
    <link href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/streamline-icon.css') }}"
          rel="stylesheet"/>
    <link href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/header.css') }}" rel="stylesheet"/>
    <link href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/portfolio.css') }}" rel="stylesheet"/>
    <link href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/blog.css') }}" rel="stylesheet"/>
    <link href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/v-animation.css') }}"
          rel="stylesheet"/>
    <link href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/v-bg-stylish.css') }}"
          rel="stylesheet"/>
    <link href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/font-icons.css') }}" rel="stylesheet"/>
    <link href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/shortcodes.css') }}" rel="stylesheet"/>
    <link href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/utilities.css') }}" rel="stylesheet"/>
    <link href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/theme-responsive.css') }}"
          rel="stylesheet"/>
    <link href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/plugins/aos/aos.css') }}"
          rel="stylesheet"/>
    <link href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/plugins/owl-carousel/owl.theme.css') }}"
          rel="stylesheet"/>
    <link href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/plugins/owl-carousel/owl.carousel.css') }}"
          rel="stylesheet"/>

    <!-- Current Page CSS -->
    <link href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/plugins/rs-plugin/css/settings.css') }}"
          rel="stylesheet"/>

    <!-- Skin -->
    <link rel="stylesheet"
          href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/skin/default.css') }}">

    <!-- Custom CSS -->
    <link rel="stylesheet" href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/custom.css') }}">

    <style type="text/css">
        :root {
            --colorTheme: {{ site.colorTheme }};
            --textColor: {{ site.textColor }};
            --headerTextColor: {{ site.lightTheme ? '#333' : 'white'  }};
            --headerBackgroundColor: {{ site.lightTheme ? 'white' : '#333'  }};
            --footerTextColor: {{ site.lightTheme ? '#333' : 'white'  }};
            --footerBackgroundColor: {{ site.lightTheme ? 'white' : '#333'  }};
        }
    </style>
    <style type="text/css">
        :root {
            --colorTheme: {{ site.colorTheme }};
            --textColor: {{ site.textColor }};
        }

         a.active {
             color: {{ site.colorTheme }} !important;
         }

          footer {
              background: {{ site.colorTheme }} !important;
              color: {{ site.textColor }} !important;
          }


        .v-pagination li a:hover,
        ul.bar-styling li:not(.selected) > a:hover,
        ul.bar-styling li > .comments-likes:hover,
        ul.page-numbers li > a:hover,
        ul.page-numbers li > span.current,
        .slideout-filter ul li.selected a,
        .btn.v-btn.v-btn-default {
            background: {{ site.colorTheme }} !important;
            color: {{ site.textColor }} !important;
        }

        .v-blog-item-info .v-btn.sf-icon-stroke i,
        .mini-v-blog-item-wrap .v-btn.sf-icon-stroke i,
        .post-inner .v-btn.sf-icon-stroke i {
            background: {{ site.colorTheme }} !important;
            color: {{ site.textColor }} !important;
        }

        .v-heading-v2 h2,
        .v-heading-v2 h3,
        .v-heading-v2 h4,
        .v-link:hover,
        .v-search-items a:hover,
        .post-info a,
        .tags-wrap .tags a,
        .v-team-member-box .read-more:hover,
        .v-tweets-slide-widget .tweet-text a {
            border-bottom-color: {{ site.colorTheme }};
        }
    </style>

    <!-- Firebrick LESS -->
    <link rel="stylesheet" href="{{ asset('templates/template-' ~ site.template.id ~ '/assets/css/izibrick.css') }}">

    <!-- Google Analytics -->
    {% if site.analyticsViewId != null %}
        <!-- Global site tag (gtag.js) - Google Analytics -->
        <script async src="https://www.googletagmanager.com/gtag/js?id={{ site.analyticsSuiviId }}"></script>
        <script>
            window.dataLayer = window.dataLayer || [];
            function gtag(){dataLayer.push(arguments);}
            gtag('js', new Date());

            gtag('config', '{{ site.analyticsSuiviId }}');
        </script>
    {% endif %}

</head>

<body class="{{ site.displayBoxed == true ? 'body' : '' }}">

<!--Header-->
<header id="header" class="header-effect-reveal"
        data-plugin-options="{'stickyEnabled': true, 'stickyEnableOnBoxed': true, 'stickyEnableOnMobile': true, 'stickyHeaderContainerHeight': 70, 'stickyStartAt': 120}">
    <div class="header-body no-shadow">
        <div class="header-container container">
            {% set siteName = site.internalName|param(app.request.server.get('REQUEST_URI')) %}
            <div class="header-row">
                <div class="header-column justify-content-start">
                    <div class="header-logo">
                        <a href="{{ path('home'|route(app.request.server.get('REQUEST_URI')), siteName) }}">
                            {% if site.logo %}
                            <img alt="Logo {{ site.name }}" height="35"
                                 src="{{ vich_uploader_asset(site, 'logoFile') | imagine_filter('logo_template') }}"/>
                            {% else %}
                                <img alt="Logo {{ site.name }}" height="35"
                                     src="{{ asset('assets/img/logo_site_default.png') }}"/>
                            {% endif %}
                        </a>
                        {% if not site.nameInLogo %}
                            <span class="logo-title">{{ site.name }}</span>
                        {% endif %}
                    </div>
                </div>
                <div class="header-column justify-content-end">
                    <div class="header-nav">
                        <div class="header-nav-main header-nav-main-effect-1 header-nav-main-sub-effect-1">
                            <nav class="collapse">
                                <ul class="nav flex-column flex-lg-row" id="mainNav">
                                    {% for pageMenu in site.pages|filter(v => v.displayMenuHeader == true)|sort((a, b) => a.menuHeaderOrder <=> b.menuHeaderOrder) %}
                                    {% if page.nameMenuUrl == pageMenu.nameMenuUrl %}
                                        <li class="active">
                                    {% else %}
                                        <li class="dropdown dropdown-mega dropdown-mega-style-2 ">
                                    {% endif %}
                                            {% set param = siteName|merge({'name':pageMenu.nameMenuUrl}) %}
                                            <a class="dropdown-item" href="{{ path('page'|route(app.request.server.get('REQUEST_URI')), param) }}">
                                                {{ pageMenu.nameMenu }}
                                            </a>
                                        </li>
                                    {% endfor %}
                                </ul>
                            </nav>
                        </div>
                        <button class="header-btn-collapse-nav ml-3" data-toggle="collapse"
                                data-target=".header-nav-main nav">
                                <span class="hamburguer">
                                    <span></span>
                                    <span></span>
                                    <span></span>
                                </span>
                            <span class="close">
                                    <span></span>
                                    <span></span>
                                </span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</header>
<!--End Header-->

<div role="main" class="main">


    {% block body %}    {% endblock %}

