    <div class="copyright">
        <div class="container">
            <p>© Copyright 2020 by <a href="https://www.izibrick.com">izibrick</a>. Tous droits réservés.</p>
            {% for pageFooter in site.pages|filter(v => v.displayMenuFooter == true)|sort((a, b) => a.menuFooterOrder <=> b.menuFooterOrder) %}
                {% set param = siteName|merge({'name':pageFooter.nameMenuUrl}) %}
                <p> {% if loop.first == false %} - {% else %}&nbsp;&nbsp;&nbsp; {% endif %} <a href="{{ path('page'|route(app.request.server.get('REQUEST_URI')), param) }}">{{ pageFooter.nameMenu }}&nbsp;</a></p>
            {% endfor %}

            <div class="clearfix pull-right">
                {% if site.facebook %}
                    <a href="{{ site.facebook }}" class="social-icon si-borderless si-facebook mb-0"
                       title="Facebook" target="_blank">
                        <i class="si-icon-facebook"></i>
                        <i class="si-icon-facebook"></i>
                    </a>
                {% endif %}
                {% if site.twitter %}
                    <a href="{{ site.twitter }}" class="social-icon si-borderless si-twitter mb-0" title="Twitter"
                       target="_blank">
                        <i class="si-icon-twitter"></i>
                        <i class="si-icon-twitter"></i>
                    </a>
                {% endif %}
                {% if site.instagram %}
                    <a href="{{ site.instagram }}" class="social-icon si-borderless si-instagram mb-0"
                       title="Instagram" target="_blank">
                        <i class="si-icon-instagram"></i>
                        <i class="si-icon-instagram"></i>
                    </a>
                {% endif %}
            </div>
        </div>
    </div>
</div>
